{
  "name": "@record/zone",
  "version": "0.5.1",
  "description": "Simplistic, promise-based zones",
  "main": "dist/index.js",
  "browser": "dist/browser.js",
  "scripts": {
    "prepublish": "npm run clean && typings install && npm run build && npm run deploy",
    "test": "ts-node node_modules/tape/bin/tape test.ts && npm run lint",
    "lint": "tslint *.ts",
    "build": "tsc",
    "dist": "npm run build && webpack dist/index.js dist/browser.js && babili -s --no-comments --compact true dist/browser.js -o dist/browser.js",
    "clean": "rm -rf dist",
    "deploy": "npm run dist"
  },
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/checle/zone.git"
  },
  "author": "Filip Dal√ºge <daluege@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/checle/zone/issues"
  },
  "homepage": "https://github.com/checle/zone",
  "devDependencies": {
    "babili": "0.0.7",
    "gh-pages": "^0.11.0",
    "tape": "^4.6.0",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "tslint-config-standard": "^1.5.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "webpack": "^1.13.1"
  }
}
